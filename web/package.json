    "cookie-parser": "^1.4.5",
    "detect-node": "^2.0.4",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "express-validator": "^6.10.0",
    "history": "^4.10.1",
    "i18next": "^19.9.2",
    "jsonwebtoken": "^8.5.1",
    "node-sass": "^4.14.1",
    "pg": "^8.5.1",
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-i18next": "^11.8.9",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
